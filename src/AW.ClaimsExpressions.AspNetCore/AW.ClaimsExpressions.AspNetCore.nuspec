<?xml version="1.0"?>
<package>
  <metadata>
    <id>MyCombinedPackage</id>
    <version>1.0.0</version>
    <authors>YourName</authors>
    <description>A combined package of Project A and Project B.</description>
    <dependencies>
      <!-- If Project B depends on A internally -->
      <dependency id="ProjectA" version="1.0.0" />
    </dependencies>
  </metadata>
  <files>
    <!-- Include Project A DLL -->
    <file src="A/bin/Release/net6.0/A.dll" target="lib/net6.0/" />
    
    <!-- Include Project B DLL -->
    <file src="B/bin/Release/net6.0/B.dll" target="lib/net6.0/" />
  </files>
</package>